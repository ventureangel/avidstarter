!!!
%html
  %head
    %title Avidstarter
    = stylesheet_link_tag    "avid", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
        
    
  %body
  .container
    .row
      .span12
        %center
          = link_to "Home", root_path
  .container
    .row
      .span12        
        %center
          - flash.keys.each do |type|
            = flash[type]
  
  = yield

